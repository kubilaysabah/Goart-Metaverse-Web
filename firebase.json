{
  "hosting": {
    "target": "goart-web",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "goart-portal-app-service",
          "region": "europe-west3"
        }
      }
    ]
  }
}
